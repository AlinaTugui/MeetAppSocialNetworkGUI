<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.lab6v2.FriendRequestsController">
<VBox alignment="CENTER" spacing="15.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <TableView fx:id="tableViewCereri">
        <columns>
            <TableColumn fx:id="tableColumnIdSender" text="IdSender" />
            <TableColumn fx:id="tableColumnStatus" text="Status" />
            <TableColumn fx:id="tableColumnTimeStamp" text="Timestamp" />
            <TableColumn fx:id="tableColumnAccept" text="Accept Request" />
            <TableColumn fx:id="tableColumnDecline" text="Decline Request" />
         <TableColumn fx:id="tableColumnUnsend" text="Unsend Request" />
        </columns>
    </TableView>
    <Button fx:id="friendRequests" onAction="#onFriendRequestsButtonClick" text="Received Friend Requests" />
   <Button fx:id="sentFriendRequests" mnemonicParsing="false" onAction="#onSentFriendRequestsButtonClick" prefHeight="25.0" prefWidth="151.0" text="Sent Friend Requests" />

</VBox>
</AnchorPane>
