<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="490.0" prefWidth="646.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.lab6v2.GroupController">
   <children>
      <TableView fx:id="tableView" layoutX="68.0" layoutY="87.0" prefHeight="374.0" prefWidth="526.0">
        <columns>
          <TableColumn fx:id="columnFirstName" prefWidth="186.0" text="FirstName" />
          <TableColumn fx:id="columnLastName" prefWidth="171.0" text="LastName" />
            <TableColumn fx:id="columnAddBtn" prefWidth="168.0" text="Add to Group" />
        </columns>
      </TableView>
      <Button fx:id="btnCreateGroup" layoutX="507.0" layoutY="40.0" mnemonicParsing="false" onAction="#createGroup" text="Create Group" />
      <TextField fx:id="groupNameField" layoutX="210.0" layoutY="40.0" prefHeight="25.0" prefWidth="226.0" promptText="Type Group Name" />
      <Label fx:id="label" layoutX="48.0" layoutY="382.0" prefHeight="38.0" prefWidth="326.0" textAlignment="CENTER" wrapText="true" />
   </children>
</AnchorPane>
